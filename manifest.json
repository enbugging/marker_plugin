{
    "manifest_version": 2,
    "name": "Chrome Extension ES6 Import Test",
    "version": "0.0.1",
    "description": "Chrome Extension ES6 Import Test",
    "author": "otiai10",
    "background": {
        "page": "src/html/background.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/js/content_script.js"]
      }
    ],
    "permissions": [
      "<all_urls>"
    ],
    "web_accessible_resources": [
      "src/js/*"
    ]
}